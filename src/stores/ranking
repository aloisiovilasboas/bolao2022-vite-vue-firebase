import { ref } from "vue";
import { defineStore } from "pinia";

export const useRankingStore = defineStore ("ranking" ,{
    state: () => {
        
        const gabarito =ref()
        const loading = ref(false)
        const ranking = ref([])
        
        const setGabarito = (newvalue) => {
            gabarito.value = newvalue;
        }
        const setLoading = (newvalue) => {
            loading.value = newvalue;
        }
        return {
            ranking,
            gabarito,
            setGabarito
        }
    }
});